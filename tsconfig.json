{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "ES2020",
    "allowJs": true,
    "moduleResolution": "node",
    "jsx": "react",
    "jsxFactory": "h",
    "strictNullChecks": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
